[run]
source = .
omit =
    */migrations/*
    */tests/*
    */venv/*
    */env/*
    */manage.py
    */wsgi.py
    */asgi.py
    */settings.py
    */urls.py
    */admin.py
    */apps.py
    */tests.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError
